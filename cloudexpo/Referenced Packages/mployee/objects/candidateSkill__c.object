{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
// Get the list of Items that should be deleted.
// Use the $ObjectType merge field to indicate the type of
// record Ids that are expected.
//
var idsToDelete = {!GETRECORDIDS( $ObjectType.candidateSkill__c)};
var deleteWarning = &apos;Are you sure you wish to delete &apos; + idsToDelete.length + &apos; skills?&apos;;

if (idsToDelete.length &amp;&amp; (window.confirm(deleteWarning))) {
// Delete the records, and pass a function into the call
// so that the toolkit refreshes the current page
// asynchronously when the call succeeds.
//
sforce.connection.deleteIds(idsToDelete,function() {
	navigateToUrl(window.location.href);});
} else if (idsToDelete.length == 0) {
	alert(&quot;Please select the Skills you wish to delete.&quot;);
}
